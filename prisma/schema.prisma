generator client {
  provider        = "prisma-client-js"
  output          = "./generated/prisma-client-js"
  previewFeatures = ["fullTextSearch", "views"]
  binaryTargets   = ["native", "darwin", "darwin-arm64", "windows"]
}

generator dbml {
  provider = "prisma-dbml-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

